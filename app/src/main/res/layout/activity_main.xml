<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="551dp"
        android:layout_height="843dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/images45"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-70dp" />

    <TextView
        android:layout_width="235dp"
        android:layout_height="84dp"
        android:text="CONSER"
        android:textColor="@color/cardview_light_background"
        android:textSize="60dp"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="134dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="CORREO ELECTRONICO"
        android:textColor="@color/cardview_light_background"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="409dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="207dp"
        android:layout_height="67dp"
        android:ems="10"
        android:foregroundGravity="center"
        android:gravity=""
        android:inputType="textPassword"
        android:text="CONTRASEÃ‘A"
        android:textColor="@color/cardview_light_background"
        android:textSize="45dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="476dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="140dp"
        android:layout_height="51dp"
        android:text="LOG IN"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="544dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="160dp"
        android:layout_height="35dp"
        android:text="Forgot Pasword"
        android:textColor="@color/cardview_light_background"
        tools:ignore="MissingConstraints"
        android:gravity="center"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="624dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="161dp"
        android:layout_height="33dp"
        android:text="Terms % Conditions"
        android:textColor="@color/cardview_light_background"
        android:gravity="center"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="682dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="47dp"
        android:layout_height="43dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="402dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="44dp"
        android:layout_height="48dp"
        app:srcCompat="@android:drawable/stat_notify_more"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="477dp" />

</androidx.constraintlayout.widget.ConstraintLayout>